# [labyrinth]

alt + w ; ctrl + d # debug
 labyrinth -d

alt + w ; e 
# normal
 labyrinth -v

alt + w ; ctrl + i
 # ins
 labyrinth -I

# [configs]

alt + q ; z
 st -e nvim ~/.zshrc

# lbyr
alt + q ; L ; {l,k}
 st -e nvim ~/{labyrinth,lbyrkmap.txt}

# lbyr
alt + q ; backslash ; {a,v,V,s,S,c,i,I,t}
 st -e nvim ~/.config/sxhkd/{audio,vim,video,system,security,config,ins,vi,task}

# widow
alt + q ; w ; {w,c,d} 
 st -e nvim {~/suckless/widow2/config.h,~/.config/warpd/config,~/.config/warpd/dunst/dunstrc}

# compositors
alt + q ; P   
 st -e nvim ~/.config/picom.conf	  

# music
alt + q ; m ; {a,A,b,B,c,d,e,m,n,N,p,r,v,x,plus}
 st -e nvim .config{/amarok/amarokrc,/audacious/audacious.conf,/beets/config.yaml,/banshee/banshee-1/banshee.conf,/cmus/config,/deadbeef/deadbeef.conf,/exaile/exaile.conf,/mpd/mpd.conf,/mopidy/mopidy.conf,/ncmpcpp/bindings,/ncmpcpp/config,/pithos/pithos.conf,/rhythmbox/rhythmbox.conf,/vlc/vlcrc,/mpprc}

alt + q ; m ; P
 st -e sudo -e /usr/share/pipewire/pipewire.conf

alt + q ; m ; M 
 st -e sudo -e /etc/mopidy/mopidy.conf

# video
alt + q ; v ; {a,b,c,d,m,n,p,P,r,v}
 st -e nvim .config{/avidemux/avidemux.conf,/baka-mplayer/config,/celluloid/config,/dragonplayer/dragonplayerrc,/mpv/mpv.conf,/nvidia/config,/Plex\ Media\ Player/config.json,/Plex\ Media\ Server/Preferences.xml,/rhythmbox/rhythmbox.conf,/vlc/vlcrc}      

# text
alt + q ; t ; {e,v}
 st -e nvim {~/.emacs,~/.vimrc}

# X
alt + q ; x ; {r,d,t}
 st -e nvim {~/.Xresources,~/.Xdefaults,~/.tmux.conf}

alt + q ; x ; m 
 st -e tmux source-file ~/.tmux.conf

# newsboat
alt + q ; r ; {n,N}
 st -e nvim {~/.newsboat/urls,~/.newsboat/config}


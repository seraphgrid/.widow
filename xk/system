# labyrinth 

alt + q ; Tab 
 sh ~/labyrinth -n 

alt + Tab
 sh ~/labyrinth -v 

alt + q ; alt + Tab 
  sh ~/labyrinth -c

alt + q ; space
 sh ~/labyrinth -d 

# [nav]

alt + w ; f
 warpd --hint

alt + w ; r
 warpd --normal

alt + w ; slash
 rofi -show window -kb-element-prev 'k' -kb-element-next 'j' -kb-accept-entry 'o'

alt + w ; I
 rofi -show drun

alt + w ; r
 spmenu

alt + q ; semicolon
 st

# [wm]

alt + q ; L  
# $hould be able to go global
 slock 

alt + w ; {j,k}
 dwmc focusstack {+,-}1

alt + {j,k}
 dwmc focusstack {+,-}1

alt + w ; {h,l}
 dwmc setmfact {-,+}0.05

shift + space ; {u,i}
 dwmc setcfact {-,+}0.85

shift + space ; o 
 dwmc setcfact

alt + w ; c 
 dwmc setlayoutex {0,1}

alt + w ; n 
 dwmc togglebar


alt + w ; {1-9}
 dwmc view {0-9}

shift + space ; {1-9}
 dwmc tagex {0-9}

alt + q ; {1-9}
 dwmc tagex {0-9}

alt + w ; {D,W}
 dwmc {pushdown,pushup} 

alt + {D,W}
 dwmc {pushdown,pushup} 

alt + w ; space 
 dwmc togglemax

alt + w ; Return
 dwmc zoom

alt + w ; e
 dwmc zoom

alt + w ; {d,w}
 dwmc focusstack {+,-}1

alt + q ; q 
 dwmc killclient

alt + w ; q 
 dwmc killclient

alt + q ; {n,N}  
 dunstctl {close-all,history-pop}


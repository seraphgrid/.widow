alt + w ; {1-9}
 dwmc view {0-9}

alt + shift + 2 ; {1-9}
 dwmc tagex {0-9}
 
alt + w ; z
 dwmc togglemax

alt + e
 hints

alt + w ; v 
 hints -m scroll

alt + w ; F 
 warpd --hint

alt + w ; r 
 warpd --normal

alt + w ; z ; z
 dwmc zoom

alt + w ; Z
 dwmc zoom

alt + w ; q 
 dwmc killclient

alt + w ; {n,N}  
 dunstctl {close-all,history-pop}

# [ss]

Print
 scrot -s '%Y-%m-%d_%H-%M-%S.png' -e 'mv $f ~/Pictures/Screenshots/' 

shift + Print
 scrot -f '%Y-%m-%d_%H-%M-%S.png' -e 'mv $f ~/Pictures/Screenshots/'

# [nav]

alt + e
 hints

alt + E 
 warpd --normal

alt + w ; v
 hints -m scroll

alt + w ; e 
 rofi -show window -kb-element-prev 'k' -kb-element-next 'j' -kb-accept-entry 'o'

alt + q ; I
 rofi -show drun


# [labyrinth]

alt + w ; i
# config
 labyrinth -I

alt + w ; a
# audio
 labyrinth -a

alt + Tab
# normal
 labyrinth -v

# [wm]

alt + w ; backslash ; q 
 dwmc quit

alt + w ; L  
 slock 

alt + {j,k}
 dwmc focusstack {+,-}1
 
alt + w ; {j,k}
  dwmc focusstack {+,-}1
 
